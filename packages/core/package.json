  "keywords": [
    "waveform",
    "audio",
    "webaudio",
    "waveform-playlist",
    "types"
  ],
  "author": "Naomi Aro",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/naomiaro/waveform-playlist.git",
    "directory": "packages/core"
  },
  "homepage": "https://naomiaro.github.io/waveform-playlist",
  "bugs": {
    "url": "https://github.com/naomiaro/waveform-playlist/issues"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {}
}
